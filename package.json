{
  "name": "how-to-react-to-your-redux",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-nibble": "^3.1.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "prettier-eslint-cli": "^3.4.3",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "fluture": "^5.0.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "sanctuary": "^0.12.2",
    "styled-components": "^1.4.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "lint:errors": "yarn lint -- --quiet",
    "lint:nibble": "./node_modules/.bin/eslint-nibble both/ client/ server/",
    "lint:fix": "yarn lint -- --fix",
    "prepush": "yarn test",
    "precommit": "yarn lint:staged",
    "format": "./node_modules/.bin/prettier-eslint \"*src/**/*.js\" --write"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add",
      "./node_modules/.bin/eslint"
    ]
  }
}
